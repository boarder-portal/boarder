html(lang=lang)
  head
    link(
      type="text/css"
      rel="stylesheet"
      href=allCSS
    )
    title#dwayne-router-title
    link#dwayne-router-icon(
      type="image/png"
      rel="shortcut icon"
    )
  body
    if NODE_ENV === 'development'
      div#livereload
    d-root(block="App")
    script(type="text/javascript").
      window.boarderUser = '#{ user }';
    script(
      type="text/javascript"
      src=i18n
    )
    script(
      type="text/javascript"
      src=allJS
    )
