tr(
  id=`room-${ id }`
  data-room-id=`${ id }`
  class=['room', `${ prefix }-room`]
)
  each col in room
    - const { name, value } = col;

    td(class=`room-${ name }`)= value
  td
    a(href=playLink)
      i.fa.fa-sign-in.enter-room
    a.observe-link(href=observeLink)
      i.fa.fa-eye.enter-room
