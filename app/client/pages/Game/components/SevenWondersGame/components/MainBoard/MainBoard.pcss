.switchHand {
  position: absolute;
  left: -140px;
  top: 150px;
  cursor: pointer;
}

.handWrapper {
  position: fixed;
  bottom: -100px;
  z-index: 24;
}

.leftArrow {
  left: -50px;
}

.rightArrow {
  right: -50px;
}

.wonderWrapper {
  position: relative;
  max-width: 500px;
}

.root {
  padding-bottom: 125px;

  .leftArrow,
  .rightArrow {
    position: absolute;
    top: 60px;
    width: 50px;
    height: 50px;
  }
}
