{
  "name": "boarder",
  "version": "2.0.0",
  "description": "",
  "default": "build/client.js",
  "scripts": {
    "dev:start": "npm-run-all --parallel dev:client:build:watch dev:server:build:watch dev:server:start:watch css:watch",
    "dev:client:build:watch": "webpack -w -c ./webpack/client.config.js",
    "dev:server:build:watch": "webpack -w -c ./webpack/server.config.js",
    "dev:server:start:watch": "supervisor ./build/server/main.js",
    "dev:analize-bundle": "ANALYZE_BUNDLE=1 npm run dev:client:build:watch",
    "prod:start": "NODE_ENV=production npm-run-all --sequential prod:client:build prod:server:build prod:server:start",
    "prod:client:build": "NODE_ENV=production webpack -c ./webpack/client.config.js",
    "prod:server:build": "NODE_ENV=production webpack -c ./webpack/server.config.js",
    "prod:server:start": "node ./build/server/main.js",
    "prod:analize-bundle": "ANALYZE_BUNDLE=1 npm run prod:client:build",
    "css:watch": "tcm -p app/**/*.pcss --watch",
    "init-db": "ts-node -r tsconfig-paths/register ./app/server/db/initDB.ts",
    "create-pexeso-set": "ts-node scripts/createPexesoSet.ts",
    "typecheck": "tsc --noEmit",
    "eslint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "eslint-fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix"
  },
  "alias": {
    "server": "./app/server",
    "client": "./app/client",
    "common": "./app/common"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/boarder-portal/boarder.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/boarder-portal/boarder/issues"
  },
  "homepage": "https://github.com/boarder-portal/boarder#readme",
  "dependencies": {
