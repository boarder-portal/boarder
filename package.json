    "createPexesoSet": "ts-node scripts/createPexesoSet.ts",
    "typecheck": "tsc --noEmit",
    "eslint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "eslintFix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "test": "jest",
    "testUpdate": "jest -u",
    "testCoverage": "jest --coverage"
  },
