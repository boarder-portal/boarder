{
  "name": "boarder",
  "version": "2.0.0",
  "description": "",
  "default": "build/client.js",
  "scripts": {
    "dev-start": "npm-run-all --parallel server-watch client-watch",
    "prod-start": "npm-run-all --sequential client-build server-start",
    "client-build": "parcel build app/client/client.tsx",
    "client-watch": "parcel watch app/client/client.tsx",
    "server-start": "ts-node --transpile-only -r tsconfig-paths/register ./app/server/server.ts",
    "server-watch": "ts-node-dev --transpile-only -r tsconfig-paths/register ./app/server/server.ts",
    "init-db": "ts-node -r tsconfig-paths/register ./app/server/db/initDB.ts",
    "create-pexeso-set": "ts-node scripts/createPexesoSet.ts",
    "typecheck": "tsc --noEmit",
    "eslint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "eslint-fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix"
  },
  "alias": {
    "server": "./app/server",
    "client": "./app/client",
    "common": "./app/common"
  },
  "browserslist": [
    "last 5 Chrome versions",
    "last 3 Safari versions",
    "last 5 Firefox versions",
    "last 3 Edge versions"
  ],
  "targets": {
    "default": {
      "context": "browser"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/boarder-portal/boarder.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/boarder-portal/boarder/issues"
  },
  "homepage": "https://github.com/boarder-portal/boarder#readme",
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@material-ui/core": "^4.9.14",
    "@material-ui/icons": "^4.9.1",
    "@types/connect-redis": "0.0.14",
    "@types/redis": "^2.8.25",
    "apollo-boost": "^0.4.7",
    "apollo-server": "^2.10.1",
    "apollo-server-express": "^2.13.1",
    "axios": "^0.20.0",
    "bem-cn": "^3.0.1",
    "connect-redis": "^5.0.0",
    "core-js": "^3.6.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "fs-extra": "^9.0.0",
    "graphql": "^14.6.0",
    "lodash": "^4.17.20",
    "multer": "^1.4.2",
    "parcel": "^2.0.0-beta.1",
    "pug": "^3.0.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "recoil": "0.0.10",
    "redis": "^3.0.2",
    "regenerator-runtime": "^0.13.3",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "styled-components": "^5.0.1",
    "styled-normalize": "^8.0.7",
    "styled-reset": "^4.1.4",
    "ts-node": "^8.6.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.7.5",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
