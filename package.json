{
  "name": "boarder",
  "version": "2.0.0",
  "description": "",
  "default": "build/client.js",
  "scripts": {
    "dev": "npm-run-all -p devServer cssWatch",
    "devServer": "ts-node-dev --transpile-only --ignore-watch /build/ -r tsconfig-paths/register ./app/server/server.ts",
    "cssWatch": "tcm -p app/**/*.pcss --watch",
    "prod": "npm-run-all -c build prodStartServer",
    "prodStartServer": "NODE_ENV=production ts-node --transpile-only -r tsconfig-paths/register ./app/server/server.ts",
    "build": "NODE_ENV=production webpack -c webpack/webpack.config.ts",
    "analyzeBundle": "ANALYZE_BUNDLE=1 NODE_ENV=production webpack -c webpack/webpack.config.ts",
    "initDb": "ts-node -r tsconfig-paths/register ./app/server/db/initDB.ts",
    "createPexesoSet": "ts-node scripts/createPexesoSet.ts",
    "typecheck": "tsc --noEmit",
    "eslint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "eslintFix": "eslint . --ext .js,.jsx,.ts,.tsx --fix"
  },
  "alias": {
    "server": "./app/server",
    "client": "./app/client",
    "common": "./app/common"
  },
